#include <iostream>


struct Person{
	int name = 1;
	void print(std::string name){
	std::cout << name << std::endl;
	}
};

int main(int argc, char **argv) {
	std::cout << "Hello, world!" << std::endl;
	Person p;
	float i = 9 / 2;
	std::cout << i << "\t" << typeid(i);
	p.print("hello");
	return 0;
}

